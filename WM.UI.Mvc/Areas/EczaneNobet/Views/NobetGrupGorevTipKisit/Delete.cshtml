@model NobetGrupGorevTipKisitDetay

@{
    ViewBag.Title = "Nöbet Grup Görev Tip Kural Sil | Nöbet Sistemi";
}

<h4>Nöbet Grup Görev Tip Kural Sil</h4>
<hr />

<h5 class="text-danger">Silmek istediğinizden emin misiniz?</h5>
<div>

    <dl class="dl-horizontal">
        <dt>
            Nöbet Grubu
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NobetGrupAdi)
        </dd>

        <dt>
            Nöbet Görev Tipi
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NobetGorevTipAdi)
        </dd>

        <dt>
            Açıklama - Üst Grup
        </dt>

        <dd>
            @Html.DisplayFor(model => model.KisitAciklama)
        </dd>

        <dt>
            Durum
        </dt>

        <dd>
            @Html.DisplayFor(model => model.PasifMi)
        </dd>

        <dt>
            Değer
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SagTarafDegeri)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.VarsayilanPasifMi)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.VarsayilanPasifMi)
        </dd>

        <dt>
            Varsayılan Değer
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SagTarafDegeriVarsayilan)
        </dd>

        <dt>
            Açıklama
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Aciklama)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Sil" class="btn btn-danger" />
            @Html.ActionLink("Listeye Dön", "Index", null, new { @class = "btn btn-warning" })
        </div>
    }
</div>
