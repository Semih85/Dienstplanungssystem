            height: 100%;
            overflow: hidden;
            overflow-y: hidden;
        }
   
        @@media (max-width: 575px) {
            /*üst sıranın tamamı, başlık tarih ve saat*/
            .div_basliklar {
                vertical-align: central;
                height: 10%;
                text-align: center;
            }
            /*Nöbetçi eczaneler yazısı div*/
            #div_labelBaslik {
                color: white;
                background-color: #ff0000;
                font-weight: 600;
            }
            /*Nöbetçi eczaneler yazısı label*/
            #lbl_baslik {
                font-size: 25px;
                width: 75%;
                height: 50%;
            }
            /*eczane logosu*/
            #eczane_logosu {
                margin-top: 3%;
                margin-left: 3%;
                width: 15%;
                height: 65%;
            }
            /*nöbet tarihinin yazıldığı üst ortadaki div*/
            #div_tarih {
                color: white;
                background-color: #ff0000;
                font-size: 26px;
            }
            /*nöbet tarihinin yazıldığı üst ortadaki label*/
            #lbl_tarih {
                font-weight: 600;
            }
            /*saatin yazıldığı üst sağdaki div*/
            #div_saat {
                color: white;
                background-color: #ff0000;
                font-size: 40px;
                font-weight: 600;
            }
            /*saatin yazıldığı label*/
            #lbl_saat {
                font-weight: 600;
            }
            /*eczanenleri adresleri ve telefonları ile yazıldığı divleri içeren soldaki sütun div*/
            #div_eczaneSolSutun {
                height: 90%;
                text-align: center;
            }

            #div_eczaneSagSutun {
                height: 90%;
                text-align: center;
            }

            .div_kucukEczaneSag {
            }
            /*eczanenleri adresleri ve telefonları ile yazıldığı soldaki divlerin hepsini classı*/
            .div_kucukEczaneSol {
            }
            /*div_kucukEczane nin içinde eczanenin adının yazıldığı label*/
            .lbl_eczane_adi {
                color: white;
                font-weight: 600;
