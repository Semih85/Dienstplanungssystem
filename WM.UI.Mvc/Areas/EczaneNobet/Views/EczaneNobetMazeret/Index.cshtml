            //nobetGrubu = getNobetGrupId();
            nobetGrubu = getNobetGrubu();

            mazeretIstekTipIdGetir();
            //alert(nobetGrubu)
            //tarihAraligi();
        });
        $("#submitSorgula").click(function (e) {

            var bitisTarihi = $('#BitisTarihi').val();
