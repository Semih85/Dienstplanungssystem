@Html.Hidden("NobetUstGrupId", (int)ViewBag.NobetUstGrupId)

<div class="row mb-2">
    <div class="col-md-4 mb-2">
        <span class="h4">Mazeret ve İstekler</span>
    </div>
    <div class="col-md-8">
        <div class="btn-group" role="group" aria-label="Basic example">
            <a class="btn btn-info btn-sm mr-4" href="@Url.Action("Create", null, new { area= "EczaneNobet" })">
                <span class="fa fa-plus">
                </span> <span class="d-none d-sm-inline"> Yeni</span> Mazeret
            </a>
            <a class="btn btn-success btn-sm mr-4" href="@Url.Action("Create", "EczaneNobetIstek", new { area= "EczaneNobet" })">
                <span class="fa fa-plus"></span>
                <span class="d-none d-sm-inline"> Yeni</span> İstek
            </a>
            <button class="btn btn-dark btn-sm" type="button" id="btnExceleAktar">
                <i class="fa fa-file-excel-o text-success"></i> Excele Aktar
                <i class="fa fa-download"></i>
            </button>
        </div>
        <button class="btn btn-warning btn-sm pull-right" type="button" data-toggle="collapse" data-target="#collapseKriterler" aria-expanded="false" aria-controls="collapseKriterler">
            <i class="fa fa-unsorted"></i> Filtre
        </button>
    </div>
</div>

