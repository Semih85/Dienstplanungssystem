            //var tarihTanim = metin.substring(0, 10).split(".");

            yil = tarihTanim[2];
            ay = tarihTanim[1];
            gun = tarihTanim[0];

            if (gun.length != 4) {
                var sonuc = yil + "." + ay + "." + gun;
                //console.log("tarihDondur2: sonra: " + sonuc)
                return sonuc;
            }
            else {
