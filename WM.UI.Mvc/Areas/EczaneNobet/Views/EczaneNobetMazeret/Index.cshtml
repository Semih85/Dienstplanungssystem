        $('#tumunuSecKaldir').click(function () {
            if (tumuSeciliMi == false && secilenGrupSayisi == 0) {
                tumuSeciliMi = true;
                $('#NobetGrupGorevTipId option').prop('selected', true);
            }
            else {
                tumuSeciliMi = false;
                $('#NobetGrupGorevTipId option').prop('selected', false);
            }
            nobetGrubu = getNobetGrubu();
        });

